{
    "name": "@qualli/qualli-rn-sdk",
    "version": "1.0.0",
    "description": "Qualli React Native SDK - Product Research on auto-pilot",
    "main": "lib/commonjs/index",
    "module": "lib/module/index",
    "types": "lib/typescript/src/index.d.ts",
    "react-native": "src/index",
    "source": "src/index",
    "files": [
        "src",
        "lib",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__",
        "android",
        "ios",
        "cpp",
        "*.podspec",
        "!ios/build",
        "!android/build",
        "!android/gradle",
        "!android/gradlew",
        "!android/gradlew.bat",
        "!android/local.properties",
        "!**/.*"
    ],
    "scripts": {
        "example": "yarn workspace qualli-rn-sdk-example",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
        "clean": "del-cli lib",
        "prepare": "bob build",
        "release": "release-it"
    },
    "keywords": [
        "react-native",
        "ios",
        "android"
    ],
    "repository": "https://github.com/Qualli-Research-Platform/qualli-rn-sdk.git",
    "author": "Nick Smet <nick@usequalli.com> (https://usequalli.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Qualli-Research-Platform/qualli-rn-sdk.git/issues"
    },
    "homepage": "https://github.com/Qualli-Research-Platform/qualli-rn-sdk.git#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    },
    "devDependencies": {
        "@babel/core": "^7.23.2",
        "@babel/preset-env": "^7.23.2",
        "@babel/runtime": "^7.23.2",
        "@commitlint/config-conventional": "^17.0.2",
        "@evilmartians/lefthook": "^1.5.0",
        "@react-native-async-storage/async-storage": "^1.19.3",
        "@react-native/eslint-config": "^0.72.2",
        "@release-it/conventional-changelog": "^5.0.0",
        "@tsconfig/react-native": "^3.0.2",
        "@types/jest": "^29.5.6",
        "@types/node": "^20.8.7",
        "@types/react-native": "0.70.0",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "babel-eslint": "^10.1.0",
        "babel-plugin-inline-dotenv": "^1.7.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "babel-plugin-root-import": "^6.6.0",
        "commitlint": "^17.0.2",
        "del-cli": "^5.0.0",
        "eslint": "^8.51.0",
        "eslint-plugin-jest": "^27.4.2",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-native": "^4.1.0",
        "metro-react-native-babel-preset": "^0.77.0",
        "prettier": "^2.4.1",
        "react": "18.2.0",
        "react-native": "0.72.5",
        "react-native-builder-bob": "^0.23.1",
        "react-native-device-info": "^10.11.0",
        "release-it": "^15.0.0",
        "typescript": "^5.1.0"
    },
    "resolutions": {
        "@types/react": "17.0.21"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*"
    },
    "workspaces": [
        "example"
    ],
    "packageManager": "yarn@3.6.1",
    "engines": {
        "node": ">= 18.0.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release ${version}",
            "tagName": "v${version}"
        },
        "npm": {
            "publish": true
        },
        "github": {
            "release": true
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "preset": "angular"
            }
        }
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            "typescript"
        ]
    },
    "dependencies": {
        "expo-device": "^5.6.0"
    }
}
