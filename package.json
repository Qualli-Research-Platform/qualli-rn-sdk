{
    "name": "@qualli/qualli-rn-sdk",
    "version": "1.0.1",
    "description": "Qualli React Native SDK - Product Research on auto-pilot",
    "main": "lib/commonjs/index",
    "module": "lib/module/index",
    "types": "lib/typescript/src/index.d.ts",
    "react-native": "src/index",
    "source": "src/index",
    "files": [
        "src",
        "lib",
        "!**/__tests__",
        "!**/__fixtures__",
        "!**/__mocks__",
        "android",
        "ios",
        "cpp",
        "*.podspec",
        "!ios/build",
        "!android/build",
        "!android/gradle",
        "!android/gradlew",
        "!android/gradlew.bat",
        "!android/local.properties",
        "!**/.*"
    ],
    "scripts": {
        "example": "yarn workspace qualli-rn-sdk-example",
        "typecheck": "tsc --noEmit",
        "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
        "lint:fix": "eslint \"src/**/*.{js,ts,tsx}\" --fix",
        "clean": "del-cli lib",
        "prepare": "bob build",
        "release": "release-it"
    },
    "keywords": [
        "react-native",
        "ios",
        "android"
    ],
    "repository": "https://github.com/Qualli-Research-Platform/qualli-rn-sdk.git",
    "author": "Nick Smet <nick@usequalli.com> (https://usequalli.com)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Qualli-Research-Platform/qualli-rn-sdk.git/issues"
    },
    "homepage": "https://github.com/Qualli-Research-Platform/qualli-rn-sdk.git#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "devDependencies": {
        "@react-native-async-storage/async-storage": "^1.19.3",
        "@react-native/eslint-config": "^0.72.2",
        "@release-it/conventional-changelog": "^7.0.2",
        "@tsconfig/react-native": "^3.0.2",
        "@types/node": "^20.8.7",
        "@types/react-native": "0.70.0",
        "del-cli": "^5.0.0",
        "eslint": "^8.51.0",
        "prettier": "^2.4.1",
        "react": "18.2.0",
        "react-native": "0.72.5",
        "react-native-builder-bob": "^0.23.1",
        "react-native-device-info": "^10.11.0",
        "release-it": "^16.2.1",
        "typescript": "^5.1.0"
    },
    "resolutions": {
        "@types/react": "17.0.21"
    },
    "peerDependencies": {
        "react": "*",
        "react-native": "*"
    },
    "workspaces": [
        "example"
    ],
    "packageManager": "yarn@3.6.1",
    "engines": {
        "node": ">= 18.0.0"
    },
    "commitlint": {
        "extends": [
            "@commitlint/config-conventional"
        ]
    },
    "release-it": {
        "git": {
            "commitMessage": "chore: release ${version}",
            "tagName": "v${version}"
        },
        "npm": {
            "publish": true
        },
        "github": {
            "release": true
        },
        "plugins": {
            "@release-it/conventional-changelog": {
                "preset": "angular"
            }
        }
    },
    "eslintIgnore": [
        "node_modules/",
        "lib/"
    ],
    "react-native-builder-bob": {
        "source": "src",
        "output": "lib",
        "targets": [
            "commonjs",
            "module",
            [
                "typescript",
                {
                    "project": "tsconfig.build.json"
                }
            ]
        ]
    }
}
