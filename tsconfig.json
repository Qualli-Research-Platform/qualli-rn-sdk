{
    "extends": "@tsconfig/react-native/tsconfig.json",
    "compilerOptions": {
        "types": ["react-native", "node"],
        "typeRoots": ["@types", "./node_modules/@types"],
        "baseUrl": ".",
        "rootDir": ".",
        "paths": {
            "@qualli/qualli-rn-sdk": ["./src/index"]
        }
    },
    "include": ["src/"],
    "exclude": [
        "node_modules",
        "babel.config.js",
        "metro.config.js",
        "jest.config.js"
    ]
}
